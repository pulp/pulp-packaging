# vim: ts=2:sw=2:et
#
# Please keep blacklist/whitelist sorted
#

[buildconfig]
builder = obal.tito_extensions.git_annex_spec_builder.GitAnnexSpecBuilder
tagger = tito.tagger.ReleaseTagger
lib_dir = rel-eng/lib/

[builder]
fetch_strategy = custom.PulpSourceStrategy

[requirements]
tito=0.6.1

[pulp-nightly-fedora27]
disttag = .fc27

[pulp-nightly-fedora26]
disttag = .fc26

[pulp-nightly-fedora25]
disttag = .fc25

[pulp-nightly-rhel7]
disttag = .el7

[pulp-nightly-rhel6]
disttag = .el6

[pulp-nightly-rhel5]
disttag = .el5
