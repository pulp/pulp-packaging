# vim: ts=2:sw=2:et
#
# Please keep blacklist/whitelist sorted
#

[buildconfig]
builder = obal.tito_extensions.git_annex_spec_builder.GitAnnexSpecBuilder
tagger = tito.tagger.ReleaseTagger
lib_dir = rel-eng/lib/

[builder]
fetch_strategy = custom.PulpSourceStrategy

[requirements]
tito=0.6.1

[pulp-2.18-fedora27]
disttag = .fc27

[pulp-2.18-fedora28]
disttag = .fc28

[pulp-2.18-rhel7]
disttag = .el7

[pulp-2.18-rhel6]
disttag = .el6

[pulp-2.18-rhel5]
disttag = .el5
